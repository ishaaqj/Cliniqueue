<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ishaaq.cliniqueue">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".loginActivity"></activity>
        <activity android:name=".contactClinicActivity" />
        <activity android:name=".waitingForConfirmationActivity" />
        <activity android:name=".createAccountUserActivity" >
            <intent-filter>
                <action android:name="com.example.ishaaq.cliniqueue"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".xtraDetailsActivity" />
        <activity android:name=".bookingConfirmationActivity" />
        <activity android:name=".loginAsAdminActivity" />
        <activity android:name=".homepageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".reasonForVisitActivity" />
    </application>

</manifest>